{
  "dependencies": {
    "mocha": "*",
    "wd": "*"
  },
  "description": "Mocha tests with SauceConnect",
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "fetch-mock": "^7.2.5",
    "node-fetch": "^2.3.0",
    "npm-run-all": "^4.1.3",
    "nyc": "^13.1.0",
    "rollup": "^0.67.1",
    "sinon": "^7.1.1",
    "static-server": "^2.2.1"
  },
  "keywords": [
    "mocha",
    "test",
    "tdd",
    "bdd",
    "saucelabs",
    "sauce connect",
    "selenium",
    "testing"
  ],
  "main": "index.js",
  "name": "mocha-sauce-connect",
  "scripts": {
    "build": "rollup -c",
    "build-watch": "rollup -cw",
    "dev-build": "run-p build-watch mocha",
    "dev": "run-s dev-build",
    "postinstall": "rollup -c",
    "mocha": "mocha -u bdd -R spec -t 5000 --watch test/tests.js",
    "mocha-coverage": "nyc --reporter=text mocha -u bdd -R spec -t 5000 --inspect test/tests.js",
    "sc": "sc",
    "test": "nyc --reporter=text mocha -u bdd -R spec -t 5000 --inspect test/tests.js"
  },
  "version": "0.0.1"
}
